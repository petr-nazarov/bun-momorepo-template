{
	"$schema": "https://turbo.build/schema.json",
	"baseBranch": "origin/main",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".next/**"]
		},
		"check": {
			"outputs": []
		},
		"fix": {
			"outputs": []
		},
		"test": {
			"outputs": []
		},
		"dev": {
			"dependsOn": ["^dev"],
			"cache": false
		}
	}
}
